document.addEventListener("DOMContentLoaded",(function(e){var n=document.getElementById("more_feed");null==n||n.addEventListener("click",(function(){var e,t,c,o;e=document.getElementById("posts_exclude_ids"),t=e.value,c=document.getElementById("feed-list"),o="/",o+="?"+new URLSearchParams({action:"hp_load_more_posts",posts_exclude:t}).toString(),n.innerHTML='<svg class="cnzd__a-ldr is--animating" x="0px" y="0px" width="16" height="16" viewBox="0 0 32 32" enable-background="new 0 0 32 32" xml:space="preserve">\n                        <circle class="cnzd__a-ldr__crcl" cx="16" cy="16" r="14"></circle>\n                    </svg>',fetch(o,{method:"GET",headers:{"Content-Type":"application/json"}}).then((function(e){return e.json()})).then((function(o){if(o)if(o.html.length>0){for(var l=o.html,d=o.post_ids,a=t.split(","),i=0;i<d.length;i++)a.push(d[i]);c.insertAdjacentHTML("beforeend",l),rw_lazyload(),document.querySelectorAll('[data-action="jslink"]').forEach((function(e){e.addEventListener("click",handleJsLinkClick)})),e.value=a.join(","),n.innerHTML='<span class="cnzd__a-btn__inr cn__body-sm cn__font--700">Voir plus d\'articles</span>'}else document.getElementById("more_feed").remove()})).catch((function(e){console.log("Request failed. ",e)}))}))}));